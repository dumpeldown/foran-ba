  title = {The Geopolitics of next Generation Mobile Communication Standardization: {{The}} Case of Open {{RAN}}},
  shorttitle = {The Geopolitics of next Generation Mobile Communication Standardization},
  author = {Kim, Mi-jin and Eom, Doyoung and Lee, Heejin},
  date = {2023-11-01},
  journaltitle = {Telecommunications Policy},
  shortjournal = {Telecommunications Policy},
  volume = {47},
  number = {10},
  pages = {102625},
  issn = {0308-5961},
  doi = {10.1016/j.telpol.2023.102625},
  url = {https://www.sciencedirect.com/science/article/pii/S0308596123001362},
  urldate = {2024-12-10},
  abstract = {The U.S. confrontation with China in technology has been escalating. One of the triggers is China's rise in 5G standardization and its lead in the telecommunications equipment market, which causes security concerns. In response, the U.S. promotes Open RAN as an alternative to the Chinese providers' proprietary solutions. It is claimed to enable the establishment and operation of open and safer networks for 5G and beyond. This paper examines why and how the U.S. promotes Open RAN and what the responses are from other countries. We find that the U.S. government's strong support for the development and dissemination of Open RAN signals its techno-nationalistic turn in international standardization. The origin of Open RAN is geopolitical in the sense that it represents a reaction to China's lead in 5G standardization. The geopolitical perspective is strengthened as we find the formation of alliances in support of Open RAN. A refreshed approach of the U.S. to standardization has introduced a new dynamic to cooperation and competition for setting the global rules on next-generation mobile communication technologies as well as emerging technologies. Standards are increasingly at the core of the ongoing geopolitical technology competition, and as such further investigation is required.},
  keywords = {5G,Geopolitics,Open RAN,Standards,Techno-nationalism},
  file = {C:\Users\Jurek\Zotero\storage\36AG5JNG\S0308596123001362.html}
}

