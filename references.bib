@article{cliffordColorCategoriesAffect2010,
  title = {Color Categories Affect Pre-Attentive Color Perception},
  author = {Clifford, Alexandra and Holmes, Amanda and Davies, Ian R.L. and Franklin, Anna},
  date = {2010-10},
  journaltitle = {Biological Psychology},
  shortjournal = {Biological Psychology},
  volume = {85},
  number = {2},
  pages = {275--282},
  issn = {03010511},
  doi = {10.1016/j.biopsycho.2010.07.014},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0301051110002139},
  urldate = {2024-11-16},
  abstract = {Categorical perception (CP) of color is the faster and/or more accurate discrimination of colors from different categories than equivalently spaced colors from the same category. Here, we investigate whether color CP at early stages of chromatic processing is independent of top–down modulation from attention. A visual oddball task was employed where frequent and infrequent colored stimuli were either same- or different-category, with chromatic differences equated across conditions. Stimuli were presented peripheral to a central distractor task to elicit an event-related potential (ERP) known as the visual mismatch negativity (vMMN). The vMMN is an index of automatic and pre-attentive visual change detection arising from generating loci in visual cortices. The results revealed a greater vMMN for different-category than same-category change detection when stimuli appeared in the lower visual field, and an absence of attention-related ERP components. The findings provide the first clear evidence for an automatic and pre-attentive categorical code for color.},
  langid = {english},
  file = {C:\Users\Jurek\Zotero\storage\APTFL42K\Clifford et al. - 2010 - Color categories affect pre-attentive color perception.pdf}
}

